# Log Debug
snippet dl
	console.log('${1}:', $1);
# Javascript Object
snippet jso
	var ${1:name} = {
		${2:key}: ${3:value}
	};
# JSON Object
snippet jo
	var ${1:name} = {
		"${1:key}": "${2:value}"	
	};
# Require
snippet rq
	var ${1:var} = require('${2:module}');
# Require lodash
snippet r_
