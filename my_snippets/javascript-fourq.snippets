# Export Module
snippet em
	module.exports = ${1:module};
# use strict
snippet us
	'use strict';
# es6: Class
snippet class
	class ${1:name} extends ${2:extends} {
		constructor(${3:arg}) {
		    super($3);
		}
	}
# 3rd party: Async.auto
snippet aa
	Async.auto({
		${1:name}: function (done) {

			${3:twerk}	
		},
		${2:name}: ['$1', function (done, results) {

			if (!results.$1) {
				return done();
			}

			done();
		}]
	}, function (err, results) {

		if (err) {
		    return callback(err);	
		}

		callback(null, results);
	});
